          <service/>
          <service-group/>
          <schedule/>
          <rulebase>
            <security>
              <rules>
                <entry name="Allow-All" uuid="2d1eb75e-7459-4c04-99eb-3a1e9a4575e4">
                  <to>
                    <member>Trust</member>
                  </to>
                  <from>
                    <member>Trust</member>
                  </from>
                  <source>
                    <member>10.0.0.0/8</member>
                    <member>172.16.0.0/12</member>
