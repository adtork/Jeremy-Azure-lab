                  <to-interface>ethernet1/1</to-interface>
                </entry>
                <entry name="NAT-All" uuid="995fb237-49fd-4e87-b331-271faf7889b3">
                  <source-translation>
                    <dynamic-ip-and-port>
                      <interface-address>
                        <ip>10.0.2.4/24</ip>
