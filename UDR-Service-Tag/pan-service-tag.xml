            <superuser>yes</superuser>
          </role-based>
        </permissions>
      </entry>
      <entry name="azureuser">
        <phash>$1$tsjdxart$ZatvKbXYPKElQmRppfxiT/</phash>
        <permissions>
          <role-based>
            <superuser>yes</superuser>
          </role-based>
        </permissions>
      </entry>
